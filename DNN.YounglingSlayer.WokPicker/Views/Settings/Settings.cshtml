@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage <DNN.WokPickerDNN.YounglingSlayer.WokPicker.Models.Settings>

@using DotNetNuke.Web.Mvc.Helpers

<h2 id="dnnSitePanel-BasicSettings" class="dnnFormSectionHead"><a href="" class="dnnSectionExpanded">@Dnn.LocalizeString("BasicSettings")</a></h2>
<fieldset>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.MultiSelect, new { @class = "col-md-2 control-label" })
        </div>
        @Html.CheckBoxFor(m => Model.MultiSelect)
    </div>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.NumberOfItems, new { @class = "col-md-2 control-label" })
        </div>
        @Html.TextBoxFor(m => Model.NumberOfItems)
    </div>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.Bvin1, new { @class = "col-md-2 control-label" })
        </div>
        @Html.TextBoxFor(m => Model.Bvin1)
    </div>
    @for (int i = 0; i < Model.cards.Count; i++)
    {
        <div class="dnnFormItem">
            <div class="dnnLabel" style="position: relative;">
                @Html.LabelFor(m => Model.cards[i].Bvin, new { @class = "col-md-2 control-label" })
            </div>
            @Html.TextBoxFor(m => Model.cards[i].Bvin)
        </div>
    }


    <div>
        <h2>section1</h2>
        <a href="@Url.Action("Section", "Settings", new {ctl = "Section", sectionId = ViewBag.section1 })"> szerkesztés  @Dnn.LocalizeString("EditItem")</a>


    </div>




</fieldset>

