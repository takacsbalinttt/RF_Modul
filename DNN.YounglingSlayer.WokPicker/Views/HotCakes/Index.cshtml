@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage<IEnumerable<DNN.WokPickerDNN.YounglingSlayer.WokPicker.Models.HotCakes>>

@using System.Text.RegularExpressions
@using DotNetNuke.Web.Mvc.Helpers
@using System.Linq
@using Dnn.Modules.Console

@{string selectedBvin = ViewBag.items.bvin;

    var item = ViewBag.items;

}


<div id="Items-@Dnn.ModuleContext.ModuleId">
    @if (Model == null)
    {
        <p>@Dnn.LocalizeString("NoItems")</p>
    }
    else
    { 
        <h2>HotCakes elemek sikeresen beolvasva!</h2>
        <h3>
            Választott bvin: @selectedBvin
        </h3>
     
        if (item == null)
        {
            <p>no cucc sry</p>
        }
        else
        {
            <p>
                @item.bvin
                <br>
                @item.SKU
                <br>
                @item.ImageFileSmall
                <br>
                Raktáron: @item.OutOfStockMode
            </p>
    
        }





    }
</div>
